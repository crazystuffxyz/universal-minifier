{"name":"universal-minifier","version":"2.0.0","description":"An all-in-one file minifier (CLI and Web Server) that drastically saves space.","main":"cli.js","bin":{"uniminify":"cli.js"},"scripts":{"start:cli":"node cli.js","start":"node server.js","bundle":"esbuild cli.js --bundle --platform=node --outfile=dist/bundle.js --external:sharp","minify":"terser dist/bundle.js --compress --mangle -o dist/bundle.min.js","build:dev":"npm run bundle && pkg dist/bundle.js --targets latest --output dist/uniminify-dev","build":"npm run bundle && npm run minify && pkg dist/bundle.min.js --targets latest-linux-x64,latest-macos-x64,latest-win-x64 --output dist/uniminify"},"repository":{"type":"git","url":"git+https://github.com/crazystuffxyz/universal-minifier.git"},"keywords":["minify","compress","cli","optimizer","server"],"author":"Shayaan Hooda","license":"MIT","bugs":{"url":"https://github.com/crazystuffxyz/universal-minifier/issues"},"homepage":"https://github.com/crazystuffxyz/universal-minifier#readme","dependencies":{"adm-zip":"^0.5.16","archiver":"^7.0.1","chalk":"^4.1.2","cli-progress":"^3.12.0","csso":"^5.0.5","express":"^5.1.0","fs-extra":"^11.3.0","gif-encoder":"^0.7.2","graphql":"^16.11.0","html-minifier-terser":"^7.2.0","image-size":"^2.0.2","jpeg-js":"^0.4.4","less":"^4.3.0","luamin":"^1.0.4","multer":"^2.0.1","php-parser":"^3.2.4","pngjs":"^7.0.0","rimraf":"^6.0.1","sass":"^1.89.2","sql-minifier":"^1.0.0","strip-comments":"^2.0.1","stylus":"^0.64.0","svgo":"^4.0.0","terser":"^5.31.3","typescript":"^5.5.4","uuid":"^11.1.0","webp-converter":"^2.3.3","webpack-glsl-minify":"^1.5.0","ws":"^8.18.3","yaml":"^2.8.0","yargs":"^17.7.2"},"devDependencies":{"esbuild":"^0.25.5","pkg":"^5.8.1","terser":"^5.43.1"}}
